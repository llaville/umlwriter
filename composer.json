{
    "name": "bartlett/umlwriter",
    "description": "Create UML class diagrams from your PHP source.",
    "keywords": ["UML", "Graphviz", "PlantUML"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/llaville/umlwriter",
    "support": {
        "source": "https://github.com/llaville/umlwriter",
        "issues": "https://github.com/llaville/umlwriter/issues"
    },
    "require": {
        "php": "^7.1.3",
        "bartlett/graph-plantuml-generator": "^1.1",
        "bartlett/graph-uml": "^1.1",
        "cweagans/composer-patches": "^1.7",
        "graphp/graph": "dev-master as 1.0.0",
        "graphp/graphviz": "dev-master as 1.0.0",
        "psr/container": "^1.0",
        "roave/better-reflection": "^4.12",
        "symfony/config": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/finder": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0"
    },
    "require-dev": {
        "jawira/plantuml": "^1.28"
    },
    "authors": [
        {
            "name": "Laurent Laville",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "bin": [
        "bin/umlwriter"
    ],
    "config": {
        "preferred-install": "source",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bartlett\\UmlWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bartlett\\UmlWriter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "enable-patching": true
    }
}
