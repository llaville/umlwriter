{
    "Verbose": false,
    "Debug": false,
    "IgnoreDefaults": false,
    "SpacesAfterTabs": false,
    "NoColor": false,
    "Exclude": [
        "\\.idea$",
        "\\.config$",
        "\\.github$",
        "vendor",
        "vendor-bin",
        "LICENSE",
        "mkdocs\\.yml$",
        "examples",
        "tests/fixtures",
        "sbom\\.json$",
        "\\.gv$",
        "\\.puml$",
        "\\.svg$",
        "\\.changie\\.yaml$",
        "\\.changes$"
